%h4= t('new_video')
= form_for(:video, url: {controller: 'videos', action: 'create' }, html: {multipart: true }) do |form|
  %table.basic-table= render partial: 'form', locals: { :f => form }
  %p.form-buttons
    = render partial: 'spree/admin/shared/edit_resource_links', locals: {collection_url: {controller: 'videos', action: 'create' }}
:javascript
  jQuery('#cancel_link').click(function (event) {
    jQuery('#new_video_link').show();
    jQuery('#video').html('');
  });